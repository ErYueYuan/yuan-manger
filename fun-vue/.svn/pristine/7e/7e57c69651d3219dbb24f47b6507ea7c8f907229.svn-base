import * as API from './'

export default {
  getOneTalkList: (params, loadings) => {
    if (process.env.NODE_ENV === 'development') { //本地环境
      return API.POST('/fzgapi/funapp/timeConversation/getOneTalkList', params, loadings)
    } else {
      return API.POST('/user/getOneTalkList', params, loadings)
    }
  },
}
