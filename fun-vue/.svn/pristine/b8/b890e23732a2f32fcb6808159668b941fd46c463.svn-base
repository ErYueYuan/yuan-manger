<script>
import Vue from 'vue'
import { api_my } from '@/api/index'
import { mapGetters } from 'vuex'
import * as tool from '@/common/Tool'
import { List } from 'vant'
Vue.use(List)
export default {
  name: 'achieveclient',
  data() {
    return {
      donelist: [],
      flag: 1,
      limit:10,
      offset:0,
      total:0,
      loading:false,
      finished:false


    }
  },
  computed: {
    ...mapGetters(['user'])
  },
  created() {
    this.getFunRenewCustomer();
  },
  mounted() {
  },
  methods: {
    changeState(val) {
      this.flag = val;
      this.getFunRenewCustomer();
    },
    goBack() {
      this.$router.go(-1);
    },
    getFunRenewCustomer() {
      let _data = {
        "agentCode": 9999,
        "saasId": "ff5a67337b6611e89feafa163eb3e537",
        "flag": this.flag,
        "offset": this.offset,
        "limit":this.limit
      }
      if(this.flag ==1){
        _data.agentCode = 9999
      }else {
        _data.agentCode = 9998
      }
      api_my.getFunRenewCustomer(_data).then(res => {
        if (res.status == tool.rtCode.status) {

          this.donelist = res.result;
        }
      })
    },
  }
}
</script>

