var api = require('../api');

module.exports = {
  getVerifyCode: function(req, res, next) {
    api.fetch(req, res, 'GET', 'fzg', 'funapp/drawImage/getVerifyCode', req.body)
  },
  checkVerifyCode: function(req, res, next) {
    api.fetch(req, res, 'GET', 'fzg', 'funapp/drawImage/verifyCode', req.body)
  },
}
