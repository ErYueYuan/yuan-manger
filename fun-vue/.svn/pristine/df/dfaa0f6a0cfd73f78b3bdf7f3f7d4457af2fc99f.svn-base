@charset "utf-8";
/* CSS Document */
*{-webkit-tap-highlight-color:rgba(255,0,0,0);-webkit-tap-highlight-color:transparent;-webkit-touch-callout: none;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,
form,fieldset,input,textarea,p,blockquote,th,td{padding:0;margin:0;font-family:"PingFangSC-Medium",Arial,sans-serif;}
body {
  background-color:#fff;
  -webkit-text-size-adjust: 100%;/*解决Iphone下橱窗字体变大问题*/
  overflow-x: hidden;
  font-size:16px;}
nav, footer, section, header, article, figure {padding: 0;margin: 0;display: block;}
ul,li {list-style: none;}
/*h1,h2,h3,h4,div,li,p,menu,dl,dt,dd{ font-size:100%; text-align:left;}影响UI插件*/
h1,h2,h3,h4{ font-weight:normal;}
a{ color:#0062b0; text-decoration:none;}
a:link {
  -webkit-tap-highlight-color: rgba(0,0,0,0);	/*禁止链接高亮*/
  /*-webkit-touch-callout: none;					禁止链接长按弹出选项菜单*/
}
::-webkit-input-placeholder {color:#999; font-size: 14px;opacity:1;}
:-moz-placeholder {color:#999; font-size: 14px;opacity:1;}
::-moz-placeholder {color:#999; font-size: 14px;opacity:1;}
:-ms-input-placeholder {color:#999; font-size: 14px;opacity:1;}

a:link, a:visited, a:hover, a:active { text-decoration:none;}
img{border:0; height:auto; width:100%;}
label{margin-bottom:0;}

pre{white-space: pre-wrap;word-wrap: break-word;}

input[type="text"]:not(.selectbox),input[type="number"],input[type="password"] {
  -webkit-user-modify: read-write-plaintext-only;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  outline-style: none;
  /*border:none;border:0;*/
}
input:disabled{ background-color: transparent;}

button{border-radius:0;}
select{ appearance:none;-webkit-appearance:none;-moz-appearance:none; border:none;background-color:transparent; outline:none;}
select.area{width: 30%;}
select.triangle{height:30px; line-height:30px;width:100%; position:absolute;top:0;right:0;bottom:0;left:0;z-index: 1;}
::-webkit-input-placeholder {color:#999; font-size: 16px;opacity:1;}
:-moz-placeholder {color:#999; font-size: 16px;opacity:1;}
::-moz-placeholder {color:#999; font-size: 16px;opacity:1;}
:-ms-input-placeholder {color:#999; font-size: 16px;opacity:1;}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {-webkit-appearance: none;}
input[type="number"] {-moz-appearance: textfield;}
option{padding:0;}
.bold{font-weight: bolder;}

input[type="text"]:not(.selectbox),input[type="number"],input[type="password"],input[type="search"] {
  -webkit-user-modify: read-write-plaintext-only;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  outline-style: none;
  /*border:none;border:0;*/
}
input[type="search"]{-webkit-appearance: none;}
input[type="search"]::-webkit-search-cancel-button{
  display: none;-webkit-appearance: none;}

textarea{ outline:none; border:0;outline-style: none; border:none;border:0; resize: none;}

