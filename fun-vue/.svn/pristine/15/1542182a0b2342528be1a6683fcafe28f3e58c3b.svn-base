<template>
  <div id="service">
    <div class="fun-top-panel bg7">
      <span class="mess-back ">
        <div
          class="cont"
          @click="goBack"
        ></div>
      </span>
      <span class="title c3 f3 dblock2 t-center " >待续费客户</span>
    </div>
    <div class="main-top p-fix">
      <section class="index-v20-top f-flex bg7 clearfix">
        <div class="sear-bb ">
          <div class=" clearfix">
            <section class="sear-c posi2 clearfix">
            </section>
          </div>
        </div>
      </section>
      <section class="nav-bar  ps bg7 border-b1">
        <div class="t-center">
          <span class="f3-1"   
            :class="type == 0?'active':''"
            @click="changeState(0)">待续费客户</span>
        </div>
        <div class="t-center">
          <span
            class="f3-1"
            :class="type == 1?'active':''"
            @click="changeState(1)"
          >保单失效客户</span>
        </div>
      </section>
    </div>
  </div>
</template>

<script>
import { api_my } from '@/api/index'
import { mapGetters } from 'vuex'
import * as tool from '@/common/Tool'
export default {
  name: 'achieveclient',
  data() {
    return {
      donelist:[],
      type:0,
    }
  },
  computed:{
    ...mapGetters(['user'])
  },
  created () {
    this.getFunRenewCustomer();
  },
  methods: {
    changeState(val){
      this.type = val;
    },
    goBack() {
      this.$router.go(-1);
    },
    getFunRenewCustomer() {
      let _data = {
        "agentCode": 2321312,
        "saasId": "ff5a67337b6611e89feafa163eb3e537",
        "flag": 1,
        "offset": 0,
        "limit": 10
      }
      api_my.getFunRenewCustomer(_data).then(res=>{
         if(res.status == tool.rtCode.status){
           this.donelist = res.result;
         }
      })
    }
  },
}
</script>

