.falg-bg input[type=range] {
  -webkit-appearance: none;
  -moz-appearance: none;
  display:inline-block;
  width:100%;
  border-radius: 6px; /*这个属性设置使填充进度条时的图形为圆角*/
  background: linear-gradient(#FD5147, #FD5147);
  background: -webkit-linear-gradient(#FD5147, #FD5147);
  background: -moz-linear-gradient(#ffffff,#ffffff);
}
.falg-bg input[type=range]:focus { outline: none;}
/*chrom */
.falg-bg input[type=range]::-webkit-slider-runnable-track {
  height: 8px;
  border-radius:6px; /*将轨道设为圆角的*/
  box-shadow:none; /*轨道内置阴影效果*/
}
.falg-bg input[type=range]::-webkit-slider-thumb {
  -webkit-appearance: none;
  height:35px;
  width: 35px;
  margin-top: -12px; /*使滑块超出轨道部分的偏移量相等*/
  background:url(../img/a3.png) 0 0 no-repeat;background-size:35px auto;
  border-radius:0px; /*外观设置为圆形*/
  border:none; /*设置边框*/
}
/*firefox */
.falg-bg input[type=range]::-moz-range-thumb {
  -webkit-appearance: none;
  height:25px;
  width: 25px;
  margin-top: -21px; /*使滑块超出轨道部分的偏移量相等*/
  background:url(../img/a3.png) 0 0 no-repeat;background-size:25px auto;
  border-radius:0px; /*外观设置为圆形*/
  border:none; /*设置边框*/
}
.falg-bg input[type=range]::-moz-range-track {
  height: 8px;
  border-radius:3px; /*将轨道设为圆角的*/
  box-shadow:none; /*轨道内置阴影效果*/
}

