.index-nav{ padding:15px;}
.index-nav span{ display: inline-block;font-family: PingFangSC-Regular;  font-size: 15px; color: #999999; margin-right: 20px;cursor:pointer;}
.index-nav span.active{ position:relative;font-family: PingFangSC-Medium;font-size: 18px;color: #FD5147;}
.index-nav span.active:after{ position: absolute; content: "";bottom:-7px;left:0;right:0;margin:0 auto;width:12px;height:6px;background-image: linear-gradient(130deg, #FF897D 0%, #FD5147 100%);
  border-radius: 0 0 100px 100px;}
.sl-other-b{ position:absolute;top:24px;left:-10px; width:135px;padding:3px; background: #FFFFFF; border-radius:6px; z-index: 5;box-shadow: 0 1px 7px 0 rgba(44,33,114,0.12);}
.sl-other-b.sob2{left:auto;right:0;}
.sl-other-b span{ display: block; cursor: pointer; line-height:50px; padding-left: 30px; text-align:left; font-size:15px; color:#666;}
.sl-other-b:before{ content:"";display: inline-block;width:12px;height:12px; background: #FFFFFF;/*box-shadow: 0 1px 7px 0 rgba(44,33,114,0.15);*/ transform: rotate(-45deg); position:absolute;top:-5px;left:13%; box-shadow:1px -1px 1px  rgba(44,33,114,0.12); }
.sl-other-b.sob2:before{left:30%;}

.timer-box{ position:relative;background: #F5F5F5;border-radius: 22px;height:44px; padding:0 10px 0 42px; display: flex; line-height: 44px;}
.timer-box:before{ position:absolute; content:"";width:28px;height:28px; background: url("../img/index/timer.png") no-repeat; background-size: 100% auto; left:8px; top:8px;}
.timer-box.no-t{padding:0 10px 0 12px;}
.timer-box.no-t:before{ display: none;}
.timer-box span{ }
.timer-box span.sp1{flex:0 0 40px;}
.timer-box span b{width:60%;height:1px; background-color: #979797; display: inline-block; vertical-align:5px;}
.timer-box .date{ position: relative;flex:1 0 auto;}
.timer-box .date:after{content:""; position: absolute; content: "";right:0px;top:18px; border-width:7px 5px 0 5px;border-style:solid; border-color:#999 transparent transparent transparent;width:0;height:0;}

.timer-box2{ position:relative;height:44px; padding:0 10px 0 2px; display: flex; line-height: 44px;}
