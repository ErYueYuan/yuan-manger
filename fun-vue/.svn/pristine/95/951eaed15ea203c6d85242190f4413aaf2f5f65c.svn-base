        created () {
            this.getTags();
        },
        methods: {
            getTags () {
                const _data = {
                    saasId:tool.app.saasId
                }
                api_sell.getTags(_data).then(data => {
                    if (data.status == tool.rtCode.status) {
                        this.intentionList = data.resultTags[0].intention;
                        this.demandList = data.resultTags[1].demand;
                        this.abilityList = data.resultTags[2].ability;
                    } else {
                        tool.toastMessage(data.message, 'error');
                    }
                });
            },



            goBack () {
                this.$router.push({ path: '/customerDetail/' + this.clientId});
            },

            
            
            
        }
    }
